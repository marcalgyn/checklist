<!DOCTYPE html>
<html lang="pt-BR">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>CheckList Atendimento v1.2.1</title>
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bulma@0.9.4/css/bulma.min.css" />
  <link href="https://cdn.jsdelivr.net/npm/bulma-calendar@6.1.18/dist/css/bulma-calendar.min.css" rel="stylesheet">
  <script src="https://kit.fontawesome.com/df8857688b.js" crossorigin="anonymous"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.mask/1.14.16/jquery.mask.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/bulma-calendar@6.1.18/dist/js/bulma-calendar.min.js"></script>

  <script>

    function desabilitarAliquota(value) {
      let input = document.querySelector("#aliquotaId");
      input.value = "";

      if (value) {
        input.disabled = false;
      } else {
        input.disabled = true;
      }
    }

    $(document).ready(function () {
      var $cnpj = $("#cnpj");
      $cnpj.mask('00.000.000/0000-00', { reverse: true });
    });

    $(document).ready(function () {
      var $telefone = $("#telefone");
      $telefone.mask('(00) 00000-0000', { reverse: false });
    });

    document.addEventListener('DOMContentLoaded', () => {
      (document.querySelectorAll('.notification .delete') || []).forEach(($delete) => {
        const $notification = $delete.parentNode;

        $delete.addEventListener('click', () => {
          $notification.parentNode.removeChild($notification);
        });
      });
    });
  </script>
</head>

<body>

  @include('layouts/partials/_navbar')

  <!-- section class="section">

  </section -->

    <div class="container" >
      @!section('content')
    </div>
</body>

</html>
